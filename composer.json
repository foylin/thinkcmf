{
    "name": "thinkcmf/thinkcmf",
    "description": "ThinkCMF based on ThinkPHP 5.1 , it is a free and open source Content Management Framework(CMF)",
    "type": "project",
    "keywords": [
    	"cmf",
    	"thinkcmf",
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://www.thinkcmf.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "thinkcmf",
            "email": "catman@thinkcmf.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "~5.1.0",
        "topthink/think-helper":"^1.0",
        "topthink/think-captcha": "^2.0",
        "phpmailer/phpmailer": "^5.2",
        "mindplay/annotations": "^1.3",
        "topthink/think-image": "^1.0",
        "qiniu/php-sdk": "^7.1",
        "dompdf/dompdf": "^0.8.0",
        "phpoffice/phpexcel": "^1.8",
        "ezyang/htmlpurifier": "^4.9",
        "topthink/think-queue": "^2.0",
        "electrolinux/phpquery": "^0.9.6"
    },
    "extra": {
        "think-path":"thinkphp"
    },
    "config": {
        "vendor-dir":"vendor"
    },
    "autoload": {
        "psr-4": {
            "cmf\\": "vendor/thinkcmf/cmf/src"
        },
        "files": [
            "vendor/thinkcmf/cmf/src/common.php"
        ]
    }
}
